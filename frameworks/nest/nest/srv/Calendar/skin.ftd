<? function skin(vars, srv) ?>
   <table class="nest_calendar">
   <tbody>
      <tr class="month_name"><td colspan="7"><? vars.skin_month(vars, srv) ?></td></tr>
      <tr class="day_names"><? for day in vars.days ?><td><?= day ?></td><? end ?></tr>
      <? for row in vars.calendar ?><tr>
         <? for day, rndr in row ?><td <? if not day: >> ' class="old"'?>><?= rndr ?></td><? end ?>
      </tr><? end ?>
   </tbody>
   </table>
<? end ?>