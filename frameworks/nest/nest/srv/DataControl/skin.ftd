<? function skin( data, srv ) ?>
<? if data.cmd ?>
   <? if data.dberror ?>
      <p><b>WARNING</b>: The database action service reported the following error:<br/>
      <b><?= data.dberror ?></b></p>
   <? else ?>
      <p>Database action <b><?= data.result ? "success" : "failure" ?></b></p>
   <? end ?>
<? end ?>
<? end ?>
