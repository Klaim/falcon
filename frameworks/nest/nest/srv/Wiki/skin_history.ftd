<? function skin( vars, srv ) ?>
   <div id="nest_wiki_location">
      <p> Location:
      <? for loc in vars.history
            page = Nest.pageLink(nil, ["wid"=>loc] )

            formiddle
               print( @"<a href=\"$page\">$loc</a>" )
               print("&nbsp;>>&nbsp;")
            end

            forlast
               printl( @"<b>$loc</b>" )
            end
         end
      ?>
      </p>
   </div>
<? end ?>