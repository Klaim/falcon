<? function skin( vars, srv, item_list, depth ) ?>
<?
   // if not rendering a specific item, use the active_items generated by the service
   if not item_list
      item_list = vars.active_items
   else
      vars.css_id = ""
   end
   cls = depth? vars.css_class +"_"+ depth : vars.css_class
?>
<ul id="<?= vars.css_id ?>" class="<?= cls ?>">
   <? for item in item_list ?>
   <li><? vars.skin_item( vars, srv, item, 0 ) ?></li>
   <? end ?>
</ul>
<? end ?>