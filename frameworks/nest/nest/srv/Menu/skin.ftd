<? function skin( vars, srv, item_list ) ?>
<?
   // if not rendering a specific item, use the active_items generated by the service
   if not item_list: item_list = vars.active_items
?>
<ul>
   <? for item in item_list ?>
   <li><? vars.skin_item( vars, srv, item ) ?></li>
   <? end ?>
</ul>
<? end ?>