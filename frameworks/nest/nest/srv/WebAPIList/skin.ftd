<? function skin( vars, srv ) ?>
<table>
   <thead>
      <tr><th>Name</th><th>description</th></tr>
   </thead>
<tbody>
   <? for func in vars.functions ?>
      <tr><td><a href="<?= func.link ?>"><?= func.name ?></td><td><?= htmlEscape(func.description) ?></td></tr>
   <? end ?>
</tbody>
</table>
<? end ?>