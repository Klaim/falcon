load hash

function Upload( params )
   // have we an old md5?
   oldmd5 = params["oldMD5"]
   
   file = Request.getField("file_one", nil)
   if not file: file = Request.getField("file_two", nil)
   file.read()
   data = file.data
   
   return ["md5" => md5( data ), "sha" => sha256(data), "oldMD5" => oldmd5 ]
end
