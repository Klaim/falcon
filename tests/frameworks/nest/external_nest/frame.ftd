<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
   "http://www.w3.org/TR/html4/strict.dtd">
<? login = Nest.service( "Login" ) ?>
<? menu = Nest.service( "Menu" ) ?>
<html>
<head>
   <title>Nest test suite - Menu service</title>
   <? Nest.headings() ?>
</head>
<body>
   <table class="header">
   <tbody>
   <tr><td class="menu"><? menu.render() ?></td><td id="login"><? login.render() ?></td></tr>
   </tbody>
   </table>

   <? Nest.content() ?>
   <hr/>
   <? if Nest.auth_level > Nest.AL.USER ?>
   <p class="gray">To login, enter "user" / "user" or "admin" / "admin".</p>
   <? end ?>
   <? Nest.service( "Menu", "footer", [ "skin" => "skin_footer.fal" ] ).render() ?>
   <p class="signature">Nest <?= Nest.getVersion() ?> test suite.</p>
</body>
</html>
