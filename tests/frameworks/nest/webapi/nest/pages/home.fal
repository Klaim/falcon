import from htmaker

stringStream = StringStream()
htmaker.OutStream.stream = stdOut(stringStream)

doc = htmaker.TransitionalFrame()

.[doc.html.add
    htmaker.Head()
    htmaker.Body()
]()

.[doc.html.head.add
    htmaker.Title(i"Web API")
    htmaker.Link("webapi.css", "stylesheet")
]()

.[doc.html.body.add
    htmaker.H1(i"API Summary")
    htmaker.Div().add(htmaker.Verbatim(Nest.service("WebAPI").render()))
]()

stringStream.close()
printl(doc.render())
