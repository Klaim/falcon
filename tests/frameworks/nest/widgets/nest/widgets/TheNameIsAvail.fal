
import from nest.widgets.NameWithAvail in na

class TheNameIsAvail() from na.NameWithAvail( "the_name", "Enter a name" )
   minSize = 4
   
   takenNames = [
      "jack",
      "john",
      "pierre",
      "luca",
      "greta",
      "gian",
      "stan"
      ]
      
   function checkAvail( name )
      return name notin self.takenNames
   end
   
end