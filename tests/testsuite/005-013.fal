/*
@name Dictionary operator sub
@category datatype

@output
Dictionary operators sub
Empty - item
Filled - not found:
values: a=1, b=2, c=3.
Filled - found:
values: b=2, c=3.
Done.
@endoutput
*/

> "Dictionary operators sub"

function testDict( dict ) 
   for a, b in dict
      forfirst: >> "values: "      
      >> a, "=", b
      formiddle: >> ", "
      forlast: >"."
   end
end

> "Empty - item"
testDict( [=>] - 'a' ) 

> "Filled - not found:"
testDict( ['a'=>1, 'b'=>2, 'c'=>3] - 'd' ) 

> "Filled - found:"
testDict( ['a'=>1, 'b'=>2, 'c'=>3] - 'a'  ) 

> "Done."
