class test
   sum = ""

   init
      list = [ "a", "b", "c", "d", "e" ]
      sum = ""
      for elem in list
          // we have called the accessor, so the list is initalized
          self.sum += elem
      end
   end
end

item = test()
if item.sum != "abcde": failure( "Summing up self with self" )
success()

