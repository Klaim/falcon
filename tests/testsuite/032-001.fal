/*
@name Basic TimeStamp
@category Feathers-timestamp

@output
Timestamp base:
1970-01-01 00:00:00.000
2002 eve: 2001-12-31 23:59:59.999
2002 first instant: 2002-01-01 00:00:00.000
1959 eve: 1958-12-31 23:59:59.999
1959 first instant: 1959-01-01 00:00:00.000
1970 eve: 1969-12-31 23:59:59.999
Epoch -1: -1
1970 first instant: 1970-01-01 00:00:00.000
@endoutput

*/

load timestamp
ts = TimeStamp()
> "Timestamp base:"
> ts

ts.year, ts.month, ts.day, ts.hour, ts.minute, ts.second, ts.msec = 2001, 12, 31, 23, 59, 59, 999
> "2002 eve: ", ts
ts.msSinceEpoch += 1
> "2002 first instant: ", ts

ts.year, ts.month, ts.day, ts.hour, ts.minute, ts.second, ts.msec = 1958, 12, 31, 23, 59, 59, 999
> "1959 eve: ", ts
ts.msSinceEpoch += 1
> "1959 first instant: ", ts

ts.year, ts.month, ts.day, ts.hour, ts.minute, ts.second, ts.msec = 1969, 12, 31, 23, 59, 59, 999
> "1970 eve: ", ts
> "Epoch -1: ", ts.msSinceEpoch
ts.msSinceEpoch += 1
> "1970 first instant: ", ts
