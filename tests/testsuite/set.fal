set = Set(1,2,3)

// in
if not (1 in set): failure( "In operator - positive" )
if 4 in set: failure( "In operator - negative" )

// notin
if 1 notin set: failure( "Notin operator - negative" )
if not 4 notin set: failure( "Notin operator - positive" )

