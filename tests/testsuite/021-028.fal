> "String methods - type checking"

tests = .[ 
   ""
   "..!;:"
   "Hello"
   "HELLO"
   "hello"
   "Hello123"
   "123"
   " \r\n\t"
   "Hi! èìùò"
]

funcs = .[
   String.isAlpha
   String.isDigit
   String.isAlphaNum
   String.isPunct
   String.isUpper
   String.isLower
   String.isWhitespace
   String.isPrintable
   String.isASCII
   String.isISO
]

function test( x )
   > "========================"
   > @"Results for ${x.describe()}"
   ^[ funcs ] {(f) > @"$({f.name}13): ", f(x) }
end

^[ tests ] test

> "Done"
