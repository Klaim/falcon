dict = [ "A" => 1, "b" => 2, "C" => 3, "d" => 4, 3 => "three", 1 => "one", 2 => "two" ]

if len( dict ) != 7: failure( "len()" )
key = "d"
dict[key] = "changed"
if dictGet( dict, key ) != "changed": failure( "Access via dictGet" )

success()

