object world
   elements = nil
   items = nil

   function add( elem )
      if self.elements == nil
         self.elements = []
         self.items = 0
      end

      self.elements += elem
      self.items++
   end

end

class alpha_class
   val = 0
   init
      world.add( self )
   end

   function setVal( param )
      self.val = param
   end
end

export

