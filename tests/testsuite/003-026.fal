function hail()
   > i"Hello"
   > i"How are you?"
end

> "I-Strings basic"
> "Untranslated:"
hail()

cp = VMProcess.current
cp.setTT( .[
   "Hello" => "Ciao!"
   "How are you?" => "Come stai?"
   ])

> "Italian:"
hail()

> "Done."
