item = [ "a" => "one", '"b"' => "two", 0 => "three" ]
str = "item[ 'a' ]"
if #str != "one": failure( "Dictionary indirect 1" )
str = "item[ '\"b\"' ]"
if #str != "two": failure( "Dictionary indirect 2" )

object test
	prop = "Hi!"
end

item["a"] = test
if # "item[ 'a' ]. prop" != "Hi!": failure( "dictionary access + object indirect" )

success()

