import Syn.*

> "Homoiconic Loop with multiple children"
> "Composing a = 0; loop ; > \"A: \", a; ++a; end a == 5"

a = 0
expr = Loop( EQ($a,5), FastPrintNL( "A: ", $a ), PreInc($a))
expr()

> "Done."
