string = "this is a string"

if not strWildcardMatch( "this is*", string ): failure( "match end" )
if not strWildcardMatch( "*a string", string ): failure( "match begining" )
if not strWildcardMatch( "this*string", string ): failure( "match middle" )

if strWildcardMatch( "*.fal", ".svn" ): failure( "partial match then fail" )


success()

