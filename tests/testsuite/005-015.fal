/*
@name Dictionary operator shr
@category datatype

@output
Dictionary operators shr
Empty - Empty
Empty - Filled:
Filled - Empty:
values: a=2, b=3, c=1.
Filled - Filled:
values: a=2, c=1.
Filled - Overwrite:
values: c=1.
Done.
@endoutput
*/

> "Dictionary operators shr"

function testDict( dict ) 
   for a, b in dict
      forfirst: >> "values: "      
      >> a, "=", b
      formiddle: >> ", "
      forlast: >"."
   end
end

> "Empty - Empty"
testDict( [=>] >> [] ) 

> "Empty - Filled:"
testDict( [=>] >> .['c' 'a' 'b'] )

> "Filled - Empty:"
testDict( ['c'=>1, 'a'=>2, 'b'=>3] >> []  ) 

> "Filled - Filled:"
testDict( ['c'=>1, 'a'=>2] >> .['b' 'd']  ) 

> "Filled - Overwrite:"
testDict( ['c'=>1, 'a'=>2] >> .['b' 'a']  ) 

> "Done."
