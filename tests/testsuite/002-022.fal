/*
@name Try/catch getting explicit items
@category Try

@output
Testing try with just body
First test
Testing try with integer raise
Second test
Integer catch: 1
Testing try with integer raise
Second test again
Integer catch (as number): 1
Testing try with String raise
Third test
String catch: Raised
Testing try with range raise
Fourth test
Range catch: [0:1:2]
Testing try with other(bool) raise
Fifth test
Other catch: false
Done
@endoutput
*/

// basic try with success
> "Testing try with just body"
try
   > "First test"
catch Integer in item
   > "Integer catch: ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end


> "Testing try with integer raise"
try
   > "Second test"
   if true
      raise 1
   end
   > "Should not be here"
catch Integer in item
   > "Integer catch: ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end

> "Testing try with integer raise"
try
   > "Second test again"
   if true
      raise 1
   end
   > "Should not be here"
catch Number in item
   > "Integer catch (as number): ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end

> "Testing try with String raise"
try
   > "Third test"
   if true
      raise "Raised"
   end
   > "Should not be here"
catch Integer in item
   > "Integer catch: ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end

> "Testing try with range raise"
try
   > "Fourth test"
   if true
      raise [0:1:2]
   end
   > "Should not be here"
catch Integer in item
   > "Integer catch: ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end

> "Testing try with other(bool) raise"
try
   > "Fifth test"
   if true
      raise false
   end
   > "Should not be here"
catch Integer in item
   > "Integer catch: ", item
catch String in item
   > "String catch: ", item
catch Range in item
   > "Range catch: ", item
catch in item
   > "Other catch: ", item
end

> "Done"
