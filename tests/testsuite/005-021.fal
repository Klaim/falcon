/*
@name Regex - "find" method

@output
Testing RE.find
R'a(.)c'.find('abcabc'): 0
R'a(.)c'.find('aaaabc'): 3
R'a(.)c'.find('bbb'): -1
R'a(.)c'.find('xxaccxaacx'): 2
R'a(.)c'.find('AAA'): -1
R'a(.)c'.find('ABC'): 0
R'a(.)c'.find('xxABCxxABC'): 2
Starting at 3
R'a(.)c'.find('abcabc',3): 3
R'a(.)c'.find('aaaabc',3): 3
R'a(.)c'.find('bbb',3): -1
R'a(.)c'.find('xxaccxaacx',3): 6
R'a(.)c'.find('AAA',3): -1
R'a(.)c'.find('ABC',3): -1
R'a(.)c'.find('xxABCxxABC',3): 7
Ending at 9
R'a(.)c'.find('abcabc',3,9): 3
R'a(.)c'.find('aaaabc',3,9): 3
R'a(.)c'.find('bbb',3,9): -1
R'a(.)c'.find('xxaccxaacx',3,9): 6
R'a(.)c'.find('AAA',3,9): -1
R'a(.)c'.find('ABC',3,9): -1
R'a(.)c'.find('xxABCxxABC',3,9): -1
@endoutput
*/

> "Testing RE.find"
regex = r'a(.)c'i
tests = .[ "abcabc" "aaaabc" "bbb" "xxaccxaacx" "AAA" "ABC" "xxABCxxABC" ]

for expr in tests
   > regex,".find('",expr,"'): ", regex.find(expr)
end

> "Starting at 3"
for expr in tests
   > regex,".find('",expr,"',3): ", regex.find(expr,3)
end

> "Ending at 9"
for expr in tests
   > regex,".find('",expr,"',3,9): ", regex.find(expr,3,9)
end
