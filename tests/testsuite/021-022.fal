/*
@name String methods - cmpi
@category Core-string

@output
String methods - endsWith
Test as a method:
"", "": true
"", "longer": false
"sho", "short": false
"some string", "": true
"some string", "ing": true
"some string", "ing ": false
"some string", "Ing", false: false
"some string", "ING", false: false
"some string", " String", true: true
"some string", " StRING", true: true
Test as a static function:
"", "": true
"", "longer": false
"sho", "short": false
"some string", "": true
"some string", "ing": true
"some string", "ing ": false
"some string", "Ing", false: false
"some string", "ING", false: false
"some string", " String", true: true
"some string", " StRING", true: true
Done
@endoutput
*/

> "String methods - cmpi"

tests = .[ 
   .["" ""]
   .["short" "longer"]
   .["STRING" "string"]
   .["strING" "StrIng"]
   .["smAllEr" "Larger"]
   .["Larger" "smAllEr"]
]

function test( code, x )
   >> ", ".merge(^[x] {(v) ^+ v.describe() } =>[]), ": "
   > code(x).describe()
end

> "Test as a method:"
^[ tests ] .[test {(x) call(x[0].cmpi, x[1:]) }]

> "Test as a static function:"
^[ tests ] .[test {(x) call( String.cmpi, x) }]

> "Done"
