x = VMContext()
> "DataDepth = ", x.dataDepth
> "Testing basic for i = 5 to 5"
for i = 5 to 5
   > "Looping: ", i
end
> "DataDepth = ", x.dataDepth
>
> "Testing basic for i = 5 to 5, -1"
for i = 5 to 5,-1
   > "Looping: ", i
end
> "DataDepth = ", x.dataDepth

>
> "Testing basic for i = 5 to 5, -2"
for i = 5 to 5,1
   > "Looping: ", i
end
> "DataDepth = ", x.dataDepth

> "Done"
