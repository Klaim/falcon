load dbi
load sample_db

d = connect()
createNamesTable( d )

>> "Querying names table"
rs = d.query( "SELECT * FROM names" )

>
> "   Row count: ", rs.getRowCount()
> "Column count: ", rs.getColumnCount()
> "Column names: ", strMerge( rs.getColumnNames(), ", " )
//> "Column types: ", reduce( lambda a,b => toString(a) + ", " + toString(b), 
//                            rs.getColumnTypes() )
//> "Column types: ", strMerge( map( toString, rs.getColumnTypes() ), ", " )
>

rs.close()

dropNamesTable( d )
d.close()
