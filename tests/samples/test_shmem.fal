load shmem


mem = SharedMem("test")
data = mem.grab()
id = data.len
> "This will be client with ID ", id
data += "A"
mem.write(data)
count = 0
while true
   >> @"$({mem.size}:r4): ",data, " \r"
   sleep(0.5)
   mem.read(data)
   data[id] = "A"/count
   if ++count == 26
      count = 0
   end
   mem.write(data)
end

   