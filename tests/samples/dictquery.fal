load dbi
load sample_db

d = connect()
createNamesTable(d)

> "Dictionary query"

nameFilter = "%"
if len( args ) > 0: nameFilter = args[0]

r = d.query( "SELECT name,dob FROM names WHERE name LIKE :1 ORDER BY dob", nameFilter )
while (a = r.fetchDict())
	> a["name"], " was born on ", a["dob"]
end
r.close()

dropNamesTable(d)

d.close()
