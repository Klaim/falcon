/****************************************************************************
* Falcon test suite
*
* ID: 40i
* Category: rtl
* Subcategory: core
* Short: numeric() test
* Description:
* Test for numeric() conversion function.
* [/Description]
*
****************************************************************************/

if numeric( 0 ) != 0: failure( "numeric( 0 )" )
if numeric( 1 ) != 1: failure( "numeric( 1 )" )
if numeric( -1 ) != -1: failure( "numeric( -1 )" )
if numeric( 0.3 ) != 0.3: failure( "numeric( 0.3 )" )
if numeric( 15.2 ) != 15.2: failure( "numeric( 15.2 )" )
if numeric( -15.2 ) != -15.2: failure( "numeric( -15.2 )" )
if numeric( 1234567891234 ) != 1234567891234: failure( "numeric( 1234567891234 )" )
if numeric( -1234567891234 ) != -1234567891234: failure( "numeric( -1234567891234 )" )

if numeric( "0" ) != 0: failure( "numeric( \"0\" )" )
if numeric( "1" ) != 1: failure( "numeric( \"1\" )" )
if numeric( "-1" ) != -1: failure( "numeric( \"-1\" )" )
if numeric( "0.3" ) != 0.3: failure( "numeric( \"0.3\" )" )
if numeric( "15.2" ) != 15.2: failure( "numeric( \"15.2\" )" )
if numeric( "-15.2" ) != -15.2: failure( "numeric( \"-15.2\" )" )
if numeric( "1234567891234" ) != 1234567891234: failure( "numeric( \"1234567891234\" )" )
if numeric( "-1234567891234" ) != -1234567891234: failure( "numeric( \"-1234567891234\" )" )

success()

